    "# my answer\n",
    "def xorOperation(n: int, start: int) -> int:\n",
    "    ret = 0\n",
    "    for i in range(n):\n",
    "        ret ^= start + 2*i\n",
    "    return ret\n",
    "\n",
    "%time xorOperation(n, start)"
   ]
